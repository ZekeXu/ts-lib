{
  "defaultSeverity": "error",
  "extends": [
      "tslint-config-airbnb",
      "tslint-plugin-prettier"
  ],
  "jsRules": {},
  "rules": {
      "indent": [true, "spaces", 2],
      "ter-indent": [true, 2],
      "ter-arrow-parens": [true, "as-needed"],
      "import-name": false,
      "max-line-length": [true, 120],
      "function-name": false,
      "space-before-function-paren": false
  },
  "rulesDirectory": []
}