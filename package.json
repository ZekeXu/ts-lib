{
  "name": "ts-lib",
  "version": "1.4.0",
  "description": "desc",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "cm": "git-cz",
    "start": "webpack-dev-server --content-base examples --mode development --config config/webpack.dev.js --hot",
    "build": "rm -rf lib && webpack --mode production --config config/webpack.lib.js",
    "format": "tslint -c tslint.json --fix -p .",
    "format-style": "prettier src/**/*.css src/**/*.scss --write && stylelint src/index.scss --fix",
    "package": "yarn pack --filename lib/ts-lib-1.0.0.tgz",
    "prepublishOnly": "yarn build",
    "release": "standard-version",
    "test": "jest",
    "coverage": "jest --coverage && cat ./coverage/lcov.info | coveralls"
  },
  "keywords": [
    "keywords"
  ],
  "author": "ZekeXu18 <zekexu18@outlook.com>",
  "license": "MIT",
  "files": [
    "lib"
  ],
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@commitlint/prompt": "^8.2.0",
    "@types/jest": "^24.0.18",
    "commitizen": "^4.0.3",
    "coveralls": "^3.0.6",
    "css-loader": "^3.2.0",
    "file-loader": "^4.2.0",
    "husky": "^3.0.5",
    "jest": "^24.9.0",
    "lint-staged": "^9.3.0",
    "mini-css-extract-plugin": "^0.8.0",
    "prettier": "^1.18.2",
    "sass": "^1.22.12",
    "sass-loader": "^8.0.0",
    "standard-version": "^7.0.0",
    "style-loader": "^1.0.0",
    "stylelint": "^11.0.0",
    "stylelint-config-recommended-scss": "^4.0.0",
    "stylelint-scss": "^3.11.1",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.1.2",
    "tslint": "^5.20.0",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.6.3",
    "url-loader": "^2.1.0",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1"
  },
  "peerDependencies": {
    "fsevents": "^2.0.1"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "yarn run format",
      "git add"
    ],
    "src/**/*.{css,scss}": [
      "yarn run format-style",
      "git add"
    ],
    "**/*.test.{ts,tsx}": [
      "yarn run test"
    ]
  },
  "standard-version": {
    "changelogHeader": "# Changelog\n\n ts-lib"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/prompt"
    }
  }
}
